{
  "name": "Remove Facebook comment emoticons",
  "version": "1.1.3",
  "manifest_version": 2,
  "description": "Switch back to older text version of emoticons.",
  "icons": {"48": "48.png", "128": "128.png"},
  "content_scripts": [ {
      "css": [ "style.css" ],
      "matches": [ "https://www.facebook.com/*", "http://www.facebook.com/*", "http://apps.facebook.com/*" ]
   }],
  "web_accessible_resources": [
    "48.png",
    "128.png"
  ],
  "permissions":[
     "http://*.facebook.com/",
     "https://*.facebook.com/",
     "https://www.facebook.com/"
  ]
}
